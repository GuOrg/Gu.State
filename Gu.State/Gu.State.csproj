<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>net45</TargetFrameworks>
    <NeutralLanguage>en-US</NeutralLanguage>
    <CodeAnalysisRuleSet>Gu.State.ruleset</CodeAnalysisRuleSet>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
  </PropertyGroup>

  <PropertyGroup>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>..\Gu.State.snk</AssemblyOriginatorKeyFile>
    <Version>0.3.0</Version>
    <Authors>Johan Larsson</Authors>
    <Description>Helper for managing state.</Description>
    <PackageProjectUrl>https://github.com/GuOrg/Gu.State/tree/master/Gu.State</PackageProjectUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/GuOrg/Gu.State/tree/master/Gu.State</RepositoryUrl>
    <PackageReleaseNotes>BUGFIXES: Multiple fixes for EqualBy
PERF: EqualBy faster now, still a lot to be done.
BREAKING: EqualByComparer no longer public.</PackageReleaseNotes>
    <PackageTags>Equals, Copy, Sync, Diff, Dirty</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <AdditionalFiles Include="PublicAPI.Shipped.txt" />
    <AdditionalFiles Include="PublicAPI.Unshipped.txt" />
  </ItemGroup>

  <Import Project="..\.paket\Paket.Restore.targets" />
</Project>